{
    "version": 2,
    "builds": [
      {
        "src": "todolist-frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "todolist-backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/todolist-backend/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/todolist-frontend/$1"
      }
    ]
  }
  